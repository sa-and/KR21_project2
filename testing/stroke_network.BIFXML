<?xml version="1.0" encoding="US-ASCII"?>

<!--Author: Ilse Feenstra, Nathan Jones, Eugenia Makarevich
    Bayesian network in XMLBIF v0.3 (BayesNet Interchange Format)
 -->

<!-- DTD for the XMLBIF 0.3 format -->
<!DOCTYPE BIF [
	<!ELEMENT BIF ( NETWORK )*>
	      <!ATTLIST BIF VERSION CDATA #REQUIRED>
	<!ELEMENT NETWORK ( NAME, ( PROPERTY | VARIABLE | DEFINITION )* )>
	<!ELEMENT NAME (#PCDATA)>
	<!ELEMENT VARIABLE ( NAME, ( OUTCOME |  PROPERTY )* ) >
	      <!ATTLIST VARIABLE TYPE (Stroke|decision|utility) "Stroke">
	<!ELEMENT OUTCOME (#PCDATA)>
	<!ELEMENT DEFINITION ( FOR | GIVEN | TABLE | PROPERTY )* >
	<!ELEMENT FOR (#PCDATA)>
	<!ELEMENT GIVEN (#PCDATA)>
	<!ELEMENT TABLE (#PCDATA)>
	<!ELEMENT PROPERTY (#PCDATA)>
]>

<BIF VERSION="0.3">
<NETWORK>
    <NAME>Stroke_network</NAME>

    <!-- Variables-->
    <VARIABLE TYPE="Stroke">
        <NAME>Smoking</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="Stroke">
        <NAME>Diabetes Type II</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="Stroke">
        <NAME>Obesity</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="Stroke">
        <NAME>High Blood Pressure</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="Stroke">
        <NAME>Stroke Symptoms</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="Stroke">
        <NAME>Acute Ischemic Stroke</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="Stroke">
        <NAME>Intracerebral Hemorrhage</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="Stroke">
        <NAME>Treatment</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="Stroke">
        <NAME>No Stroke</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="Stroke">
        <NAME>Survival</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
 
    <!-- PCTs -->
    <DEFINITION>
        <FOR>Smoking</FOR>
        <TABLE>0.206 0.794</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Diabetes Type II</FOR>
        <TABLE>0.0602 0.938</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Obesity</FOR>
        <TABLE>0.4 0.6</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>High Blood Pressure</FOR>
        <GIVEN>Smoking</GIVEN>
        <GIVEN>Obesity</GIVEN>
        <TABLE>0.90 0.1 0.75 0.25 0.75 0.25 0.25 0.75</TABLE> <!-- Order: Rain?, Sprinkler?, Wet Grass?-->
    </DEFINITION>
    <DEFINITION>
        <FOR>Stroke Symptoms</FOR>
        <GIVEN>Obesity</GIVEN>
        <GIVEN>Diabetes Type II</GIVEN>
        <GIVEN>High Blood Pressure</GIVEN>
        <TABLE>0.01662 0.98338 0.01242 0.98758 0.011183 0.986975 0.005437 0.994563 0.013025 0.996405 0.003595 0.988817 0.00420 0.9958 0.003388 0.996612</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Intracerebral Hemorrhage</FOR>
        <GIVEN>Stroke Symptoms</GIVEN>
        <TABLE>0.13 0.77 0.1 1.0</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Acute Ischemic Stroke</FOR>
        <GIVEN>Stroke Symptoms</GIVEN>
        <TABLE>0.87 0.03 0.1 1.0</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>No Stroke</FOR>
        <GIVEN>Stroke Symptoms</GIVEN>
        <TABLE>0.1 0.9 1.0 0</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Treatment</FOR>
        <GIVEN>Intracerebral Hemorrhage</GIVEN>
        <GIVEN>Acute Ischemic Stroke</GIVEN>
        <TABLE>0.98 0.02 0.98 0.02 0.98 0.02 0.02 0.98</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Survival</FOR>
        <GIVEN>Treatment</GIVEN>
        <GIVEN>No Stroke</GIVEN>
        <TABLE>0.99 0.01 0.6 0.4 0.95 0.05 0.01 0.99</TABLE>
    </DEFINITION>
</NETWORK>
</BIF>